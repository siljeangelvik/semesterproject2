{
  "name": "vite-tailwind",
  "private": false,
  "description": "auction application client",
  "version": "0.0.0",
  "keywords": [
    "auction",
    "app",
    "client",
    "javascript",
    "vite",
    "tailwind"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "npm run build & npm run tailwind:watch & vite preview --port 8080",
    "serve": "vite serve",
    "Serve": "npm run build & npm run tailwind & vite preview",
    "tailwind": "npx tailwindcss -i src/css/*.{css,scss} -o dist/css/*.{css,scss}",
    "tailwind:watch": "npx tailwindcss -i src/css/input.css -o dist/css/output.css --watch",
    "serve:all": "npm run build & npm run tailwind & npm run dev",
    "clean:build": "vite build src --emptyOutDir"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.19",
    "sass": "^1.56.1",
    "tailwindcss": "^3.2.4",
    "vite": "^3.2.3",
    "vite-plugin-dynamic-import": "^1.2.4"
  },
  "dependencies": {
    "@rollup/plugin-dynamic-import-vars": "^2.0.1",
    "glob": "^8.0.3"
  },
  "author": "Silje Angelvik",
  "license": "ISC",
  "homepage": "https://github.com/siljeangelvik/semesterproject2#readme",
  "bugs": {
    "url": "https://github.com/siljeangelvik/semesterproject2/issues"
  }
}
